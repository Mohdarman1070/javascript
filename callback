callback is function where function is passed as an argument





const calculate = (a, b, operation) =>{
    return operation(a, b);

}


 const a = calculate(5, 7, function(num1, num2){
    return num1 + num2;
})
console.log(a)



function multiply(a, b){
    return a * b;
}

const b = calculate(4, 8, multiply);
console.log(b)



const sub = (a, b) => a- b;
const c = calculate(112, 23, sub);
console.log(c)
